Object subclass: #Criterio	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Clases'!"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Criterio class	instanceVariableNames: ''!!Criterio class methodsFor: 'as yet unclassified' stamp: '4/18/2024 21:36'!crearCriterioMas: unCriterio	|criterio|	unCriterio = 'Fuerte' ifTrue: [ criterio := CriterioFuerte new ].	unCriterio = 'Debil' ifTrue: [ criterio := CriterioDebil new ].	criterio isNil ifTrue: [ CriterioNoValidoException signal ].	^criterio.! !Criterio subclass: #CriterioDebil	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Clases'!!CriterioDebil methodsFor: 'as yet unclassified' stamp: '4/19/2024 07:48'!buscarPeleadorEnListaDeRastreos: historial	| peleadorMinimo |	peleadorMinimo := historial detectMin: [ :peleador |		                  peleador obtenerNivelDePelea ].	peleadorMinimo ifNil: [		PeleadorNoRegistradoException signal:			'No hay Rastreos, rastrea antes de Buscar un peleador' ].	^ peleadorMinimo obtenerNombre! !Object subclass: #Modelo	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Clases'!"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Modelo class	instanceVariableNames: ''!!Modelo class methodsFor: 'as yet unclassified' stamp: '4/19/2024 07:48'!crearModelo: unModelo 	| modelo |	unModelo = 'Nuevo' ifTrue: [ modelo := ModeloNuevo new ].	unModelo = 'Viejo' ifTrue: [ modelo := ModeloViejo new ].	modelo ifNil: [ModeloNoValidoException signal: 'El Modelo "', unModelo , '" no está registrado.'].	^modelo! !Object subclass: #Rastreo	instanceVariableNames: 'nombre nivelDePelea modeloQueEscaneo transformacion'	classVariableNames: ''	package: 'TP1-Clases'!!Rastreo methodsFor: 'as yet unclassified' stamp: '4/16/2024 16:53'!ki: unKi 	nivelDePelea := unKi.! !!Rastreo methodsFor: 'as yet unclassified' stamp: '4/18/2024 21:50'!conNombre: unNombre ki: unKi transformacion: unaTranformacion yModeloDeEscaneo: unModelo 	nombre := unNombre.	nivelDePelea := unKi.	transformacion := unaTranformacion.	modeloQueEscaneo := unModelo.	! !!Rastreo methodsFor: 'as yet unclassified' stamp: '4/16/2024 16:17'!es: unNombre	^ (nombre = unNombre)! !!Rastreo methodsFor: 'as yet unclassified' stamp: '4/16/2024 17:08'!obtenerNombre		^nombre.! !!Rastreo methodsFor: 'as yet unclassified' stamp: '4/16/2024 16:42'!obtenerNivelDePelea	^nivelDePelea.! !!Rastreo methodsFor: 'accessing' stamp: '4/16/2024 16:52'!nombre: unNombre 	nombre := unNombre.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Rastreo class	instanceVariableNames: ''!!Rastreo class methodsFor: 'as yet unclassified' stamp: '4/18/2024 21:48'!conNombre: aString ki: anInteger transformacion: aKaioKen yModeloDeEscaneo: aModeloNuevo 	| rastreo |		rastreo := Rastreo new.	^rastreo conNombre: aString ki: anInteger transformacion: aKaioKen yModeloDeEscaneo: aModeloNuevo.! !!Rastreo class methodsFor: 'as yet unclassified' stamp: '4/19/2024 01:48'!conNombre: unNombre yKi: unKi	| rastreo |	rastreo := self new.	rastreo nombre: unNombre.	rastreo ki: unKi.	^rastreo! !Error subclass: #PeleadorNoRegistradoException	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Clases'!Object subclass: #AlgoRastreadorZ	instanceVariableNames: 'historial criterio'	classVariableNames: ''	package: 'TP1-Clases'!!AlgoRastreadorZ methodsFor: 'as yet unclassified' stamp: '4/19/2024 04:37'!registrarRastreoConModelo: unModelo delPeleadorConNombre: unNombre KiBase: unKi yTransformacion: unaTransformacion	| transformacion modelo comprobante |	"Crea el modelo"	modelo := Modelo crearModelo: unModelo.	transformacion := Transformacion crearTransformacion: unaTransformacion.		comprobante := modelo rastrearPeleadorConNombre: unNombre KiBase: unKi yTransformacion: transformacion.	historial add: comprobante.	! !!AlgoRastreadorZ methodsFor: 'as yet unclassified' stamp: '4/18/2024 21:36'!nivelDePeleaDe: unNombre    	|rastreo|		[ rastreo := (historial detect: [ :comprobante | comprobante es: unNombre ]) ] on: NotFound do:[ PeleadorNoRegistradoException signal: 'El peleador "', unNombre , '" no está registrado.'.	].		^rastreo obtenerNivelDePelea.	! !!AlgoRastreadorZ methodsFor: 'as yet unclassified' stamp: '4/19/2024 07:48'!obtenerPeleadorSegunCriterio	^criterio buscarPeleadorEnListaDeRastreos: historial.	! !!AlgoRastreadorZ methodsFor: 'initialization' stamp: '4/16/2024 16:44'!initialize 	historial := OrderedCollection new.! !!AlgoRastreadorZ methodsFor: 'initialization' stamp: '4/18/2024 21:51'!conCriterioMas: unCriterio 	criterio := unCriterio.! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!AlgoRastreadorZ class	instanceVariableNames: ''!!AlgoRastreadorZ class methodsFor: 'as yet unclassified' stamp: '4/18/2024 21:51'!conCriterioMas: unCriterio	| rastreador criterio|	rastreador := self new.		criterio := Criterio crearCriterioMas: unCriterio.	rastreador conCriterioMas: criterio.	^ rastreador! !Error subclass: #CriterioNoValidoException	instanceVariableNames: 'historial criterio'	classVariableNames: ''	package: 'TP1-Clases'!Modelo subclass: #ModeloViejo	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Clases'!!ModeloViejo methodsFor: 'as yet unclassified' stamp: '4/17/2024 20:19'!rastrearPeleadorConNombre: unNombre KiBase: unKi yTransformacion: unaTransformacion	| ki rastreo |	ki := unaTransformacion transformarKi: unKi.	ki := ki min: 9000.	rastreo := Rastreo conNombre: unNombre yKi: ki.	^ rastreo! !Object subclass: #Transformacion	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Clases'!!Transformacion methodsFor: 'as yet unclassified' stamp: '4/19/2024 04:36'!comprobarKi: unKi	(unKi > 0) ifFalse: [ KiNoValidoException signal: 'El Ki tiene que ser mayor a 0'].! !"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!Transformacion class	instanceVariableNames: ''!!Transformacion class methodsFor: 'as yet unclassified' stamp: '4/19/2024 07:49'!crearTransformacion: unaTransformacion 	| transformacion |	unaTransformacion = 'Base' ifTrue: [ transformacion := Base new ].	unaTransformacion = 'Kaio-Ken' ifTrue: [ 		transformacion := KaioKen new ].	unaTransformacion = 'Mono Gigante' ifTrue: [ 		transformacion := MonoGigante new ].	transformacion ifNil: [TransformacionNoValidaException signal: 'La transformacion "', unaTransformacion , '" no es valida.'].	^transformacion! !Transformacion subclass: #KaioKen	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Clases'!!KaioKen methodsFor: 'as yet unclassified' stamp: '4/19/2024 04:38'!transformarKi: unKi	self comprobarKi: unKi.	^ 2*unKi! !Transformacion subclass: #MonoGigante	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Clases'!!MonoGigante methodsFor: 'as yet unclassified' stamp: '4/19/2024 04:38'!transformarKi: unKi	self comprobarKi: unKi.	^(unKi*10) asLargerPowerOfTwo! !Error subclass: #ModeloNoValidoException	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Clases'!Error subclass: #KiNoValidoException	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Clases'!Criterio subclass: #CriterioFuerte	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Clases'!!CriterioFuerte methodsFor: 'as yet unclassified' stamp: '4/19/2024 07:48'!buscarPeleadorEnListaDeRastreos: aCollection 	| peleadorMaximo |	peleadorMaximo := aCollection detectMax: [ :peleador|  peleador obtenerNivelDePelea ].	peleadorMaximo ifNil: [		PeleadorNoRegistradoException signal:			'No hay Rastreos, rastrea antes de Buscar un peleador' ].	^ peleadorMaximo obtenerNombre	! !Modelo subclass: #ModeloNuevo	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Clases'!!ModeloNuevo methodsFor: 'as yet unclassified' stamp: '4/18/2024 21:45'!rastrearPeleadorConNombre: unNombre KiBase: unKi yTransformacion: unaTransformacion	| ki rastreo |		ki := unaTransformacion transformarKi: unKi.	rastreo := Rastreo conNombre: unNombre ki: ki transformacion: unaTransformacion yModeloDeEscaneo: self.	^rastreo.	! !Transformacion subclass: #Base	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Clases'!!Base methodsFor: 'as yet unclassified' stamp: '4/19/2024 04:38'!transformarKi: unKi 	self comprobarKi: unKi.	^unKi ! !Error subclass: #TransformacionNoValidaException	instanceVariableNames: ''	classVariableNames: ''	package: 'TP1-Clases'!